package com.company;
public class Main {

    public static void main(String[] args) {
       int number = 9;
        System.out.println(SumDigits(number));



    }

    public static int SumDigits(int number){
        int sum = 0;
        if(number< 10){
            return -1;
        }

        while(number != 0){
            sum+=number%10;
            number = number/10;

        }
        return sum;


    }


}
